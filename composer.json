{
	"name" : "avored/graphql",
	"description" : "AvoRed Laravel Shopping Cart Graphql API",
	"keywords" : [
		"graphql",
		"api",
		"cart",
		"laravel",
		"e commerce",
		"laravel5",
		"shop",
		"shopping-cart",
		"e-commerce",
		"shopping cart",
		"e commerce"
	],
	"license" : "MIT",
	"authors" : [{
			"name" : "Purvesh  ",
			"email" : "ind.purvesh@gmail.com"
		}
	],
	"type" : "library",
	"require" : {
		"php" : ">=7.1.3",
		"avored/framework": "3.*",
		"laravel/passport": "^7.3",
		"rebing/graphql-laravel": "^2.0"
	},
	"require-dev" : {
		"phpunit/phpunit": "^7.5",
		"orchestra/testbench" : "~3.7",
		"mockery/mockery" : "1.2.*"
