			"AvoRed\\Graphql\\Tests\\" : "tests/"
		}
	},
	"extra" : {
		"laravel" : {
			"providers" : [
				"AvoRed\\Graphql\\GraphqlProvider"
			]
		}
	},
	"homepage" : "https://www.avored.com",
